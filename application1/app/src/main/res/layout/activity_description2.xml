<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="description2VM" type="com.application.app.modules.description2.data.viewmodel.Description2VM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearDescription2"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearGroup216"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_27pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameGroup218"
                android:layout_height="@dimen/_90pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangle2"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_90pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY" android:src="@drawable/img_rectangle2"/>
                <LinearLayout android:id="@+id/linearGroup210"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearGroup221"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.69"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_height="@dimen/_47pxh"
                            android:layout_width="@dimen/_42pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_menu"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDETAILS"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginStart="@dimen/_31pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_weight="0.66"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{description2VM.description2Model.txtDETAILS}"
                            style="@style/txtUbuntubold20" tools:text="@string/lbl_details"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageProfile31"
                        android:layout_height="@dimen/_63pxh"
                        android:layout_marginStart="@dimen/_152pxh"
                        android:layout_width="@dimen/_71pxh"
                        android:scaleType="fitXY" circular="@{true}"
                        imageRes="@{@drawable/img_profile31}" tools:src="@drawable/img_profile31"/>
                </LinearLayout>
            </FrameLayout>
            <ImageView android:id="@+id/imageUpsc11"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_223pxh"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="@dimen/_226pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_upsc11_1}" tools:src="@drawable/img_upsc11_1"/>
            <LinearLayout android:id="@+id/linearGroup211"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingStart="@dimen/_5pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:text="@{description2VM.description2Model.txtDescription}"
                    style="@style/txtSolidRoundedOutline_3" tools:text="@string/lbl_description"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnTestPattern"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_14pxh"
                    android:paddingStart="@dimen/_14pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_test_pattern"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_test_pattern"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAboutTheExam"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{description2VM.description2Model.txtAboutTheExam}"
                style="@style/txtUbuntubold20_1" tools:text="@string/lbl_about_the_exam"/>
            <TextView android:gravity="start"
                android:id="@+id/txtUnionPublicSe"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:text="@{description2VM.description2Model.txtUnionPublicSe}"
                style="@style/txtUbuntubold15_1" tools:text="@string/msg_union_public_se"/>
            <FrameLayout android:id="@+id/frameGroup220"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_44pxv"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh" android:layout_width="@dimen/_200pxh">
                <ImageView android:id="@+id/imageRectangle242"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_44pxh"
                    android:layout_width="@dimen/_200pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_rectangle242_1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAPPLY"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_51pxh"
                    android:layout_marginStart="@dimen/_51pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{description2VM.description2Model.txtAPPLY}"
                    style="@style/txtUbuntubold20" tools:text="@string/lbl_apply"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>

<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="androidLarge14VM" type="com.application.app.modules.androidlarge14.data.viewmodel.AndroidLarge14VM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearAndroidLarge"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearGroup245"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameGroup248"
                android:layout_height="@dimen/_90pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangle2"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_90pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY" android:src="@drawable/img_rectangle2"/>
                <LinearLayout android:id="@+id/linearGroup241"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearGroup253"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.69"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_height="@dimen/_47pxh"
                            android:layout_width="@dimen/_42pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_menu"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDETAILS"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginStart="@dimen/_31pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_weight="0.66"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{androidLarge14VM.androidLarge14Model.txtDETAILS}"
                            style="@style/txtUbuntubold20" tools:text="@string/lbl_details"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageProfile31"
                        android:layout_height="@dimen/_63pxh"
                        android:layout_marginStart="@dimen/_152pxh"
                        android:layout_width="@dimen/_71pxh"
                        android:scaleType="fitXY" circular="@{true}"
                        imageRes="@{@drawable/img_profile31}" tools:src="@drawable/img_profile31"/>
                </LinearLayout>
            </FrameLayout>
            <ImageView android:id="@+id/imageUpsc11"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_223pxh"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="@dimen/_226pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_upsc11_1}" tools:src="@drawable/img_upsc11_1"/>
            <LinearLayout android:id="@+id/linearGroup242"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingStart="@dimen/_5pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:text="@{androidLarge14VM.androidLarge14Model.txtDescription}"
                    style="@style/txtSolidRoundedOutline_1" tools:text="@string/lbl_description"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnTestPattern"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_14pxh"
                    android:paddingStart="@dimen/_14pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_test_pattern"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline_1" tools:text="@string/lbl_test_pattern"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPatternForThe"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_53pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{androidLarge14VM.androidLarge14Model.txtPatternForThe}"
                style="@style/txtUbuntubold20_1" tools:text="@string/msg_pattern_for_the"/>
            <FrameLayout android:id="@+id/frameGroup250"
                android:layout_gravity="start"
                android:layout_height="@dimen/_166pxv"
                android:layout_marginEnd="@dimen/_31pxh"
                android:layout_marginStart="@dimen/_31pxh"
                android:layout_marginTop="@dimen/_45pxv" android:layout_width="@dimen/_287pxh">
                <FrameLayout android:id="@+id/frameGroup252"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_166pxv" android:layout_width="@dimen/_287pxh">
                    <TextView android:id="@+id/txtPreliminaryExa"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:layout_width="match_parent"
                        android:text="@{androidLarge14VM.androidLarge14Model.txtPreliminaryExa}"
                        style="@style/txtUbuntubold20_1" tools:text="@string/msg_preliminary_exa"/>
                    <ImageView android:id="@+id/imageRectangle242"
                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_44pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_200pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_rectangle242_3"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAPPLY"
                    android:layout_gravity="bottom|end"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_67pxh"
                    android:layout_marginStart="@dimen/_67pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{androidLarge14VM.androidLarge14Model.txtAPPLY}"
                    style="@style/txtUbuntubold20" tools:text="@string/lbl_apply"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>

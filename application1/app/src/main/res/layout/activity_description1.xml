<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="description1VM" type="com.application.app.modules.description1.data.viewmodel.Description1VM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearDescription1"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearGroup201"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameGroup204"
                android:layout_height="@dimen/_90pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangle2"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_90pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY" android:src="@drawable/img_rectangle2"/>
                <LinearLayout android:id="@+id/linearGroup197"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearGroup209"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.68"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_height="@dimen/_47pxh"
                            android:layout_width="@dimen/_42pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_menu"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDETAILS"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_weight="0.66"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{description1VM.description1Model.txtDETAILS}"
                            style="@style/txtUbuntubold20" tools:text="@string/lbl_details"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageProfile31"
                        android:layout_height="@dimen/_63pxh"
                        android:layout_marginStart="@dimen/_160pxh"
                        android:layout_width="@dimen/_71pxh"
                        android:scaleType="fitXY" circular="@{true}"
                        imageRes="@{@drawable/img_profile31}" tools:src="@drawable/img_profile31"/>
                </LinearLayout>
            </FrameLayout>
            <ImageView android:id="@+id/imageGate33"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_131pxh"
                android:layout_marginEnd="@dimen/_58pxh"
                android:layout_marginStart="@dimen/_60pxh"
                android:layout_marginTop="@dimen/_74pxv"
                android:layout_width="@dimen/_300pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_gate33}" tools:src="@drawable/img_gate33"/>
            <LinearLayout android:id="@+id/linearGroup198"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_34pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingStart="@dimen/_10pxh"
                    android:paddingTop="@dimen/_16pxv"
                    android:text="@{description1VM.description1Model.txtDescription}"
                    style="@style/txtSolidRoundedOutline_1" tools:text="@string/lbl_description"/>
                <TextView android:id="@+id/txtTestPattern"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_24pxv"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:text="@{description1VM.description1Model.txtTestPattern}"
                    style="@style/txtSolidRoundedOutline_2" tools:text="@string/lbl_test_pattern"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPatternForThe"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_60pxh"
                android:layout_marginStart="@dimen/_60pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{description1VM.description1Model.txtPatternForThe}"
                style="@style/txtUbuntubold20_1" tools:text="@string/msg_pattern_for_the"/>
            <FrameLayout android:id="@+id/frameGroup206"
                android:layout_gravity="start"
                android:layout_height="@dimen/_166pxv"
                android:layout_marginEnd="@dimen/_60pxh"
                android:layout_marginStart="@dimen/_60pxh"
                android:layout_marginTop="@dimen/_40pxv" android:layout_width="@dimen/_269pxh">
                <FrameLayout android:id="@+id/frameGroup208"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_166pxv" android:layout_width="@dimen/_269pxh">
                    <TextView android:id="@+id/txtAptitudeTechn"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:text="@{description1VM.description1Model.txtAptitudeTechn}"
                        style="@style/txtUbuntubold15" tools:text="@string/msg_aptitude_techn"/>
                    <ImageView android:id="@+id/imageRectangle242"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_44pxh"
                        android:layout_marginEnd="@dimen/_33pxh"
                        android:layout_marginStart="@dimen/_33pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_200pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_rectangle242"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAPPLY"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_90pxh"
                    android:layout_marginStart="@dimen/_90pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{description1VM.description1Model.txtAPPLY}"
                    style="@style/txtUbuntubold20" tools:text="@string/lbl_apply"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
